                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFit" horizontalHuggingPriority="251" verticalHuggingPriority="251" fixedFrame="YES" image="King" translatesAutoresizingMaskIntoConstraints="NO" id="qIU-AK-Bf9">
